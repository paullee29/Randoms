package practice;

public class Practice {

	public static void main(int[] args) {
		int [] a = {2, 4, 3, 5, 1};
		
		// TODO Auto-generated method stub
	    int bestindex = a.length+1;
	    int currentindex = -1;
	    int dup = -1;
	    for (int i :a) {
	        for (int x : a) {
	            int z = 0;
	            if (i==x) {
	                dup = i;
	                currentindex = z;
	                if (currentindex<bestindex){
	                    bestindex = z;
	                }                   
	            }
	            z += 1;
	        }
	        
	        
	    }
	    System.out.println(dup);

	}

}
